{
  "name": "pubg-tracker",
  "version": "1.0.0",
  "description": "Track player stats and matches for Player Unknown Battle Grounds",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "build:css": "node-sass --include-path scss ./src/scss/index.scss --output-style expanded ./dist/build/public/client/index.css",
    "heroku-postbuild": "webpack --config webpack.config.js --mode production && node-sass --include-path scss ./src/scss/index.scss --output-style expanded ./dist/build/public/client/index.css"
  },
  "engines": {
    "node": "v8.11.1",
    "npm": "6.13.1"
  },
  "author": "Brandon Blackwell",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "history": "^4.10.1",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.0",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "node-sass": "^4.14.0",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
